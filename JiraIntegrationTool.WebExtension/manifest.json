{
  "name": "Jira integration tool",
  "version": "1.0",
  "description": "Jira integration tool",
  "manifest_version": 2,
  "permissions": [
    "nativeMessaging",
    "notifications",
    "storage"
  ],
  "optional_permissions": [
    "<all_urls>"
  ],
  "background": {
    "scripts": [
      "polyfill/content-scripts-register-polyfill.js",
      "polyfill/browser-polyfill.js",
      "scripts/background.js"
    ]
  },
  "web_accessible_resources": [
    "scripts/pagescript.js"
  ],
  "options_ui": {
    "page": "pages/options/options.html",
    "browser_style": true
  },
  "applications": {
    "gecko": {
      "id": "{f6309bf7-aaab-43a3-8f67-dd4b32224cfa}",
      "strict_min_version": "50.0"
    }
  },
  "key": ""
}
